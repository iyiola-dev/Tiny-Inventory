{
  "name": "tiny-inventory-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:routes": "vitest run test/*.test.ts",
    "test:services": "vitest run test/services/*.test.ts",
    "test:integration": "npm run test:services",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "migrate": "npm run db:migrate",
    "seed": "npm run db:seed"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "fastify": "^4.28.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.8.1",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.24.0",
    "pino-pretty": "^11.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  }
}
